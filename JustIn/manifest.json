{
    "manifest_version" : 3,
    "name" : "JustIn",
    "version": "1.0.0",
    "permissions": [
      "activeTab", "storage"
    ],
    "content_scripts": [
        {
          "matches": ["https://www.linkedin.com/*"],
          "js": ["content.js"]
        }
    ],
    "icons": { "240": "yes.png"} ,
    "description": "LinkedIn Assistant",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "yes.png",
            "48": "yes.png",
            "128": "yes.png"
        }
    }
}